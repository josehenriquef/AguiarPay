@model IEnumerable<AguiarPay.Business.Models.ComandaIndividual>

@{
    ViewData["Title"] = "Index";
}

<h1>Gerenciar comandas individuais</h1>

<p>
    <a asp-action="Create">Criar uma nova</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                <a>Comanda @item.Id</a>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
